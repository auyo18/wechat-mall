/* pages/home/home.wxss */

.swiper {
  width: 100%;
  height: 370rpx;
}

.swiper image {
  width: 100%;
  height: 100%;
}

.menus {
  display: flex;
  flex-wrap: wrap;
}

.menus .item {
  width: 20%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.menus .item image {
  width: 100rpx;
  height: 100rpx;
  margin-bottom: 10rpx;
}

.menus .item text {
  font-size: 26rpx;
}
